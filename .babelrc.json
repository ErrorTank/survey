{
  "presets": [
    "@babel/preset-env", "@babel/preset-react"],
  "sourceMaps": true,
  "plugins": [
    ["@babel/plugin-transform-template-literals", {
      "loose": true
    }],

    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-proposal-class-properties", "@babel/plugin-syntax-dynamic-import", "@babel/plugin-proposal-optional-chaining", "@babel/plugin-proposal-throw-expressions"]
}