*,
*::after,
*::before {
  margin: 0;
  padding: 0;
  box-sizing: inherit;
}

@keyframes spinner{
  from{
    transform rotateZ(0)
  }
  to{
    transform rotateZ(360deg)
  }
}
.ellipse-text{
  text-overflow ellipsis
  overflow hidden
  white-space nowrap
}
.common-table-container{
  position relative
  .ctc-table{
    margin-top 1rem
  }
}
body {
  font-family: "Roboto", sans-serif;
  box-sizing: border-box;
  overflow-x hidden
  margin 0
  padding 0
  font-weight: 300;
  line-height: 1.6;
  background-color: #f2f2f2 !important;
  position: relative;
  z-index -1
}

#app{
  min-height: 100vh;
  position relative
  z-index: -1;
  background: url("./static/image/thumb-1920-916086.jpg") no-repeat center center fixed;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  &:after {
    content : "";
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color rgba(black, 0.8)
    z-index -1


  }

}