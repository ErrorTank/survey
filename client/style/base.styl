*,
*::after,
*::before {
  margin: 0;
  padding: 0;
  box-sizing: inherit;
}

@keyframes spinner{
  from{
    transform rotateZ(0)
  }
  to{
    transform rotateZ(360deg)
  }
}
body {
  font-family: "Roboto", sans-serif;
  box-sizing: border-box;
  overflow-x hidden
  margin 0
  padding 0
  font-weight: 300;
  line-height: 1.6;
  background-color: #f2f2f2 !important;
  position: relative;
}

#app{
  min-height: 100vh;

}